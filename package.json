{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Web client",
  "author": "Agave Lab",
  "license": "ISC",
  "main": "server/server.isomorphic",
  "scripts": {
    "start-prod": "better-npm-run start-prod",
    "start-dev": "better-npm-run start-dev",
    "start-test": "better-npm-run start-dev-test",
    "build": "better-npm-run build",
    "test-e2e": "nightwatch",
    "test-e2e-headless-desktop": "nightwatch --env headless-desktop",
    "test-e2e-headless-tablet": "nightwatch --env headless-tablet",
    "linter": "./node_modules/.bin/eslint ./src/js",
    "linter-fix": "./node_modules/.bin/eslint --fix ./src/js"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./server/server.isomorphic",
      "env": {
        "NODE_PATH": "./src",
        "PORT": 4000,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "webpack-dev-server --host 0.0.0.0 --config ./webpack/dev.config.js",
      "env": {
        "CSS_MODULES": "true",
        "API_HOST": "http://localhost:3000/",
        "PORT": 4000
      }
    },
    "start-dev-test": {
      "command": "webpack-dev-server --host 0.0.0.0 --config ./webpack/dev.config.js",
      "env": {
        "CSS_MODULES": "true",
        "API_HOST": "http://api:3000/",
        "PORT": 4000
      }
    },
    "build": {
      "command": "webpack --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "0.0.15",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^0.10.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-strip-code": "^1.0.4",
    "image-webpack-loader": "^3.2.0",
    "load-grunt-configs": "^1.0.0",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.13",
    "node-sass": "^4.9.0",
    "postcss-loader": "^1.3.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.2",
    "susy": "^2.2.12",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "airbnb-js-shims": "^1.1.1",
    "autoprefixer": "^6.7.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-react-css-modules": "^3.3.0",
    "babel-plugin-transform-react-display-name": "^6.23.0",
    "blacklist": "^1.1.4",
    "classnames": "^2.2.5",
